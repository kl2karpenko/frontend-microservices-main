    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "7.23.0",
    "prettier": "2.2.1",
    "ts-essentials": "7.0.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3",
    "@types/jest": "^26.0.20",
    "@types/prop-types": "15.7.3",
