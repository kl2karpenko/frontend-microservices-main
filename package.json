{
  "name": "fe-microservices-example",
  "private": true,
  "bolt": {
    "workspaces": [
      "modules/*"
    ]
  },
  "scripts": {
    "start": "nodemon -x 'npm run build' -w src -e ts,tsx",
    "test": "cross-env NODE_ENV=test jest",
